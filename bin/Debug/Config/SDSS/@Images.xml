		 	 </WMSAccessor>
			</ImageAccessor>
			<ExtendedInformation>
				<ToolBarImage>add-ons\sdss\sdss\blob.png</ToolBarImage>
			</ExtendedInformation>
		</QuadTileSet>

	</ChildLayerSet>
	<ChildLayerSet Name="Footprint Imagery" ShowOnlyOneLayer="false" ShowAtStartup="true">
		<QuadTileSet ShowAtStartup="false">
			<Name>SDSS Footprint</Name>
			<DistanceAboveSurface>0</DistanceAboveSurface>
			<BoundingBox>
				<North>
					<Value>90.</Value>
				</North>
				<South>
					<Value>-90.</Value>
				</South>
				<West>
					<Value>-180.</Value>
				</West>
				<East>
					<Value>180.</Value>
				</East>
			</BoundingBox>
      <Opacity>160</Opacity>
      <TerrainMapped>false</TerrainMapped>
      <ImageAccessor>
        <LevelZeroTileSizeDegrees>36.0</LevelZeroTileSizeDegrees>
        <NumberLevels>11</NumberLevels>
        <TextureSizePixels>512</TextureSizePixels>
        <ImageFileExtension>jpeg</ImageFileExtension>
        <WMSAccessor>
          <ServerGetMapUrl>http://nvogre.phyast.pitt.edu/cgi-bin/nvomapserv</ServerGetMapUrl>
          <ServiceName>WMS</ServiceName>
          <Version>1.1.1</Version>
          <ImageFormat>image/jpeg</ImageFormat>
          <WMSLayerName>sdsslevel0,sdsslevel1,sdsslevel2&amp;TRANSPARENT=true</WMSLayerName>
          <UseTransparency>true</UseTransparency>
          <CacheExpirationTime>
            <Days>0</Days>
            <Hours>12</Hours>
            <Mins>0</Mins>
            <Seconds>0</Seconds>
          </CacheExpirationTime>
          <ServerLogoFilePath>add-ons\sdss\sdss\gal-footprint.png</ServerLogoFilePath>
        </WMSAccessor>
      </ImageAccessor>
    <TransparentColor><Red>0</Red><Green>0</Green><Blue>0</Blue></TransparentColor>

			<ExtendedInformation>
				<ToolBarImage>add-ons\sdss\sdss\gal-footprint.png</ToolBarImage>
			</ExtendedInformation>
		</QuadTileSet>

