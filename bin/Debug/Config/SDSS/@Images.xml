<LayerSet Name="Sky" ShowOnlyOneLayer="false" ShowAtStartup="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="LayerSet.xsd">

	<ChildLayerSet Name="Survey Imagery" ShowOnlyOneLayer="false" ShowAtStartup="true">
		<QuadTileSet ShowAtStartup="true">
			<Name> Sloan Digital Sky Server</Name>
			<DistanceAboveSurface>0</DistanceAboveSurface>
			<BoundingBox>
				<North>
					<Value>90.</Value>
				</North>
				<South>
					<Value>-90.</Value>
				</South>
				<West>
					<Value>-180.</Value>
				</West>
				<East>
					<Value>180.</Value>
				</East>
			</BoundingBox>
			<TerrainMapped>false</TerrainMapped>
			<ImageAccessor>
				<LevelZeroTileSizeDegrees>36.</LevelZeroTileSizeDegrees>
				<NumberLevels>21</NumberLevels>
				<TextureSizePixels>512</TextureSizePixels>
				<ImageFileExtension>jpeg</ImageFileExtension>
		 	<WMSAccessor>
		 	  <ServerGetMapUrl>http://nvogre.phyast.pitt.edu/cgi-bin/nvomapserv</ServerGetMapUrl>
		 	  <ServiceName>WMS</ServiceName>
		 	  <Version>1.1.1</Version>
		 	  <ImageFormat>image/jpeg</ImageFormat>
	 		  <WMSLayerName>sdssimgl0,sdssimgl1,sdssimgl2,sdssimgl3</WMSLayerName>
	 		  <UseTransparency>true</UseTransparency>
			  <Opacity>250</Opacity>
	 		  <ServerLogoFilePath>add-ons\sdss\sdss\sdss.png</ServerLogoFilePath>
		 	 </WMSAccessor>
			</ImageAccessor>
			<ExtendedInformation>
				<ToolBarImage>add-ons\sdss\sdss\sdss.png</ToolBarImage>
			</ExtendedInformation>
		</QuadTileSet>


		<QuadTileSet ShowAtStartup="false">
