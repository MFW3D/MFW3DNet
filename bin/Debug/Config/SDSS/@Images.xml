				<TextureSizePixels>512</TextureSizePixels>
				<ImageFileExtension>jpeg</ImageFileExtension>
		 	<WMSAccessor>
		 	  <ServerGetMapUrl>http://nvogre.phyast.pitt.edu/cgi-bin/nvomapserv</ServerGetMapUrl>
		 	  <ServiceName>WMS</ServiceName>
		 	  <Version>1.1.1</Version>
		 	  <ImageFormat>image/jpeg</ImageFormat>
	 		  <WMSLayerName>wmap0</WMSLayerName>
	 		  <UseTransparency>true</UseTransparency>
			  <Opacity>100</Opacity>
